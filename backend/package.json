{
  "name": "ai-study-circle-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Study Circle platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seedTemplates.js",
    "dynamo:setup": "node scripts/dynamo-setup.js setup",
    "dynamo:list": "node scripts/dynamo-setup.js list",
    "dynamo:delete": "node scripts/dynamo-setup.js delete",
    "build": "echo 'No build step required for backend'",
    "logs:setup": "node setup-logging.js",
    "logs:clear": "rm -rf logs/*.log logs/archive/*",
    "logs:rotate": "find logs -name \"*.log\" -type f -exec gzip {} \\; && mv logs/*.gz logs/archive/",
    "logs:tail": "tail -f logs/combined-$(date +%Y-%m-%d).log",
    "logs:errors": "tail -f logs/error-$(date +%Y-%m-%d).log"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.454.0",
    "@aws-sdk/lib-dynamodb": "^3.454.0",
    "@google-cloud/translate": "^8.0.2",
    "archiver": "^6.0.1",
    "aws-sdk": "^2.1498.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "docx": "^8.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "franc": "^6.1.0",
    "google-translate-api-x": "^10.7.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-microsoft": "^1.0.0",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.13.0",
    "transliteration": "^2.3.5",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "ai",
    "api",
    "education",
    "backend"
  ],
  "author": "Your Name",
  "license": "MIT"
}